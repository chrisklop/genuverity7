{
  "version": 2,
  "functions": {
    "api/index.py": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/infographics/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/((?!infographics/).*)",
      "destination": "/index.html"
    }
  ]
}
